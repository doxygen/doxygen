     if (a>b) b=a;
     outlineParser()->addVhdlType(m_sharedState->current->name,b,EntryType::makeVariable(),
                                  VhdlSpecifier::GENERIC,ss,QCString(),Protection::Public);
   }
   m_sharedState->currP=VhdlSpecifier::UNKNOWN;return QCString();
 }
}


