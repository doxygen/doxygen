  <compounddef id="concept_hashable" kind="concept">
    <compoundname>Hashable</compoundname>
    <templateparamlist>
      <param>
        <type>typename T</type>
      </param>
    </templateparamlist>
    <initializer>template&lt;typename T&gt;
concept <ref refid="concept_hashable" kindref="compound">Hashable</ref> =  requires(T a)
{
    /** For values `a` of type `T`, the expression `std::hash&lt;T&gt;{}(a)`
     *  should compile and its result is convertible to `std::size_t` */
    { std::hash&lt;T&gt;{}(a) } -&gt; std::convertible_to&lt;std::size_t&gt;;
}</initializer>
